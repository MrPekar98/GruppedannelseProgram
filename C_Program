#include <stdio.h>

/* Enumerations */
enum type_t = {visionary = 1, idealist, dirigent, troubleshooter};
typedef enum type_t PersonType;

/* Structs */
/* Belbin struct */
struct BelbinTestData
{
    int Plant;
    int MonitorEvaluator;
    int Specialist;
    int Shaper;
    int Implementer;
    int CompleterFinisher;
    int Coordinator;
    int Teamworker;
    int ResourceInvestigator;
};

/* Main struct: person struct */
struct person
{
    char *name;
    char sex; Det vil Martin have med hjælpe læren <3
    
    /* Belbin struct declaration */
    struct BelbinTestData belbin;
};
typefed struct person PersonData;

/* Protocols */

/* Main function */
int main()
{
    int AmountOfStudents, GroupSize;
    
    printf("Amount of students: ");
    int test = scanf("%d", &AmountOfStudents);
    int test2 = scanf("%d", &GroupSize);
    
    if (test != 1 || test2 != 1)
    {
        printf("Format exception.\n");
        exit(EXIT_FAILURE);
    }
    
    /* Data array: Containing all information about every student. */
    PersonData persons[AmountOfStudents];
    
    return 0;
}
